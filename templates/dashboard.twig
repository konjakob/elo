{% extends 'layout_dashboard.twig' %}

{% block default %}


<div class="uk-grid uk-grid-divider uk-grid-medium uk-child-width-1-2 uk-child-width-1-4@l uk-child-width-1-5@xl" data-uk-grid>
	{% for d in data %}
	<div>
		<span class="uk-text-small"><span data-uk-icon="icon:{{ d.icon }}" class="uk-margin-small-right uk-text-primary"></span>{{ d.title }}</span>
		<h1 class="uk-heading-primary uk-margin-remove  uk-text-primary">{{ d.total }}</h1>
		<div class="uk-text-small">
			<span class="uk-text-{% if d.change > 0 %}success{% else %}warning{% endif %}" data-uk-icon="icon: {% if d.change > 0 %}triangle-up{% else %}triangle-down{% endif %}">{{ d.change }}</span> more than last week.
		</div>
	</div>
	{% endfor %}
</div>
<hr>
<div class="uk-grid uk-grid-medium" data-uk-grid>
	
	<!-- panel -->
	<div class="uk-width-1-2@l">
		<div class="uk-card uk-card-default uk-card-small uk-card-hover">
			<div class="uk-card-header">
				<div class="uk-grid uk-grid-small">
					<div class="uk-width-auto"><h4>Sales Chart</h4></div>
					<div class="uk-width-expand uk-text-right panel-icons">
						<a href="#" class="uk-icon-link" title="Move" data-uk-tooltip data-uk-icon="icon: move"></a>
						<a href="#" class="uk-icon-link" title="Configuration" data-uk-tooltip data-uk-icon="icon: cog"></a>
						<a href="#" class="uk-icon-link" title="Close" data-uk-tooltip data-uk-icon="icon: close"></a>
					</div>
				</div>
			</div>
			<div class="uk-card-body">
				<div class="chart-container">
					<canvas id="chart2"></canvas>
				</div>
			</div>
		</div>
	</div>
